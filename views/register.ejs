  <body>
    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg navbar-dark fixed-top" id="mainNav">
      <div class="container">
        <a class="navbar-brand js-scroll-trigger" href="/">Cargo Bikes</a>
      </div>
    </nav>

    <div class="container">
      <form class="form-signin" method="post" action="/register/post">
        <h2 class="form-signin-heading">Please register</h2>
        <div class="form-group">
          <label for="user">Name</label>
          <input type="text" class="form-control" id="name" name="name">
        </div>
        <div class="form-group">
          <label for="user">Email</label>
          <input type="text" class="form-control" id="email" name="email">
        </div>
        <div class="form-group">
          <label for="user">Username</label>
          <input type="text" class="form-control" id="username" name="username">
        </div>
        <div class="form-group">
          <label for="user">Type of user</label>
          <select class="form-control" id="type" name="type">
            <option value="CargoOwner">CargoOwner</option>
            <option value="Carrier">Carrier</option>
          </select>
        </div>
        <div class="form-group">
          <label for="user">Password</label>
          <input type="password" id="inputPassword" class="form-control" id="password" name="password">
        </div>
        <div class="form-group">
          <label for="user">Confirm password</label>
          <input type="password" class="form-control" id="password2" name="password2">
        </div>
        <button type="submit" class="btn btn-lg btn-primary btn-block" type="submit">Register</button>
        <input type="reset" class="btn btn-lg btn-primary btn-block">
      </form>
    </div>
  </body>
</html>
